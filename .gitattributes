*.max  diff=lfs merge=lfs -text
*.mb  diff=lfs merge=lfs -text
*.obj  diff=lfs merge=lfs -text
*.ply  diff=lfs merge=lfs -text
*.skp  diff=lfs merge=lfs -text
*.stl  diff=lfs merge=lfs -text
*.ztl  diff=lfs merge=lfs -text
